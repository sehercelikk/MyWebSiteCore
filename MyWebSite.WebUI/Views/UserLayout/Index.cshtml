@{
    Layout = null;
}


<!DOCTYPE html>
<html lang="en">
@await Component.InvokeAsync("_UserHeadComponentPartial")

<body class="index-page">
    @await Component.InvokeAsync("_UserHeaderComponentPartial")


    <main class="main">
        @RenderBody()
        <!-- Hakkimda Section -->
        @await Component.InvokeAsync("_UserAboutComponentPartial")
        <!-- /hakkimda Section -->


        <!-- Egitim Section -->
        @await Component.InvokeAsync("_UserEducationComponentPartial")
        <!-- /Egitim Section -->


        <!-- Deneyim Section -->
        @await Component.InvokeAsync("_UserExperienceComponentPartial")
        
        <!-- /Deneyim Section -->


        <!-- Beceriler Section -->
        @await Component.InvokeAsync("_UserSkillComponentPartial")
        
        <!-- /Beceriler Section -->

        <!-- Hobby Section -->
        @await Component.InvokeAsync("_UserHobbyComponentPartial")
        
        <!-- /Hobby Section -->

        <!-- Sertificate Section -->
        @await Component.InvokeAsync("_UserSertificateComponentPartial")
        
        <!-- /Sertificate Section -->


        <!-- Portfolio Section -->
        @await Component.InvokeAsync("_UserProjectComponentPartial")
        

        <!-- /Portfolio Section -->
        <!-- Contact Section -->
        @await Component.InvokeAsync("_UserContactComponentPartial")
        
        <!-- /Contact Section -->


    </main>

    <footer id="footer" class="footer position-relative">

        <div class="container">
            <div class="copyright text-center ">

                <p>© <span>Copyright</span> @DateTime.Now.Year <span>Tüm Hakları Saklıdır</span></p>
            </div>
            <div class="credits">
                Geliştirici <a href="#">Seher ÇELİK</a>
            </div>
        </div>

    </footer>

    <!-- Scroll Top -->
    <a href="#" id="scroll-top" class="scroll-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

    <!-- Preloader -->
    <div id="preloader"></div>

    <!-- Vendor JS Files -->
    <script src="/Sablon/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="/Sablon/assets/vendor/php-email-form/validate.js"></script>
    <script src="/Sablon/assets/vendor/aos/aos.js"></script>
    <script src="/Sablon/assets/vendor/typed.js/typed.umd.js"></script>
    <script src="/Sablon/assets/vendor/purecounter/purecounter_vanilla.js"></script>
    <script src="/Sablon/assets/vendor/waypoints/noframework.waypoints.js"></script>
    <script src="/Sablon/assets/vendor/isotope-layout/isotope.pkgd.min.js"></script>
    <script src="/Sablon/assets/vendor/imagesloaded/imagesloaded.pkgd.min.js"></script>
    <script src="/Sablon/assets/vendor/glightbox/js/glightbox.min.js"></script>
    <script src="/Sablon/assets/vendor/swiper/swiper-bundle.min.js"></script>

    <!-- Main JS File -->
    <script src="/Sablon/assets/js/main.js"></script>

</body>

</html>